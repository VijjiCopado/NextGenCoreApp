    <listViews>
        <fullName>AllAccounts</fullName>
        <columns>ACCOUNT.NAME</columns>
        <columns>HCSC_Division__c</columns>
        <columns>Market_Segment__c</columns>
        <columns>Renewal_Date__c</columns>
        <columns>ACCOUNT.ACCOUNT_NUMBER</columns>
        <columns>External_ID__c</columns>
        <columns>ACCOUNT.CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Forecast_Segment__c</field>
            <operation>equals</operation>
            <value>Small Group</value>
        </filters>
        <filters>
            <field>ZZ_ACA_Metallic_Count__c</field>
            <operation>greaterOrEqual</operation>
            <value>1</value>
        </filters>
        <filters>
            <field>Renewal_Date__c</field>
            <operation>equals</operation>
            <value>7/1/2019</value>
        </filters>
        <filters>
            <field>HCSC_Division__c</field>
            <operation>equals</operation>
            <value>IL,TX,NM,OK,MT</value>
        </filters>
        <label>All Small Group Accounts</label>
        <sharedTo>
            <role>Small_Group</role>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>All_Hold_Configurations</fullName>
        <columns>ACCOUNT.NAME</columns>
        <columns>ACCOUNT.ACCOUNT_NUMBER</columns>
        <columns>Hold_Reasons__c</columns>
        <columns>hold_end_date__c</columns>
        <columns>renewal_duration_is_infinite__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>hold_end_date__c</field>
            <operation>greaterThan</operation>
            <value>TODAY</value>
        </filters>
        <label>All Hold Configurations</label>
        <sharedTo>
            <role>UnderwriterUser</role>
        </sharedTo>
    </listViews>
    <validationRules>
        <fullName>SGR_Hold_account</fullName>
        <active>false</active>
        <description>Enter hold end date or check the checkbox if hold reason is provided.</description>
        <errorConditionFormula>IF( AND(OR(
  AND((ISBLANK(hold_end_date__c)),renewal_duration_is_infinite__c=False,NOT(ISBLANK(SGR_Hold_Reason__c))), 
  AND(NOT(ISBLANK(hold_end_date__c)),renewal_duration_is_infinite__c=TRUE,ISBLANK(SGR_Hold_Reason__c)),
  AND((hold_end_date__c &gt;= today()),renewal_duration_is_infinite__c=False,(ISBLANK(SGR_Hold_Reason__c))),
  AND(NOT(ISBLANK(hold_end_date__c)),renewal_duration_is_infinite__c=TRUE,NOT(ISBLANK(SGR_Hold_Reason__c)))),(ISPICKVAL(Market_Segment__c, &apos;SG&apos;))) , TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Please enter either Hold end date or Check infinite hold checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SGR_Other_hold_reason</fullName>
        <active>false</active>
        <description>If miscellaneous is selected than other reason should be provided.</description>
        <errorConditionFormula>IF(
AND(INCLUDES(SGR_Hold_Reason__c,&apos;Miscellaneous&apos;),ISBLANK(SGR_Other_Hold_Reasons__c)), TRUE, False)</errorConditionFormula>
        <errorMessage>Please provide other hold reasons</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SGR_holdreason</fullName>
        <active>true</active>
        <description>If end hold date is present hold reason cannot be empty.</description>
        <errorConditionFormula>IF(AND
(OR(
    AND((hold_end_date__c &gt; today()),ISBLANK(SGR_Hold_Reason__c)),
    AND(ISBLANK(hold_end_date__c),renewal_duration_is_infinite__c=TRUE,ISBLANK(SGR_Hold_Reason__c)),
    AND (ISPICKVAL(Status__c,&apos;Underwriter hold&apos;),ISBLANK(SGR_Hold_Reason__c))),(ISPICKVAL(Market_Segment__c, &apos;SG&apos;))) , TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Hold Reason Cannot be empty if Hold End Date Greater or Equal to Current Date. Please enter Hold Reason.</errorMessage>
    </validationRules>
</CustomObject>
